{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/functions/v1/:path*",
      "destination": "https://kvauqrxihwdgclhwmzof.supabase.co/functions/v1/:path*"
    },
    {
      "source": "/auth/avito-callback(.*)",
      "destination": "/auth/avito-callback$1"
    },
    {
      "source": "/",
      "destination": "/landing/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/landing",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/landing/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
