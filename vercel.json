{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/functions/v1/:path*",
      "destination": "https://kvauqrxihwdgclhwmzof.supabase.co/functions/v1/:path*"
    },
    {
      "source": "/auth/avito-callback",
      "destination": "/index.html"
    },
    {
      "source": "/landing",
      "destination": "/landing/index.html"
    },
    {
      "source": "/landing/:path*",
      "destination": "/landing/index.html",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/landing/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/landing/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
