{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/functions/v1/(.*)",
      "destination": "https://kvauqrxihwdgclhwmzof.supabase.co/functions/v1/$1"
    },
    {
      "source": "/auth/avito-callback",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/:path*",
      "destination": "/app/index.html"
    },
    {
      "source": "/landing/:path*",
      "destination": "/landing/index.html"
    }
  ],
  "headers": [
    {
      "source": "/landing/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/app/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
